{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "outDir": "example-app-dist",
    "target": "ES5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true
    // "lib": [
    //   "es2019"
    // ]
  },
  "include": [
    "./src/**/*",
    "./example-app/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true,
}